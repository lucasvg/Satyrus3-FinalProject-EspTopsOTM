#prec: 30;
#epsilon: 1.5E-08;
#dir: "c:/Users/Pedro";

m = 3;
n = m + 2;

#exit: 0;

x[m] = {(1) : 1, (m) : -1};
y[n] = {(1) : 0, (n) : +1};

%{---------------------}
  * THIS IS SOME MULTI-
  * LINE COMMENT...
 {---------------------}%

% Integrity Constraints
(int) A:
  @{i=[1:m]} % forall i from 1 to m
  ${j=[1:n]} % exists j from 1 to n

  x[i] -> y[j];

% Optmality Constraints
(opt) X[1]:
  @{i=[1:m]}
  @{j=[1:n], i != j}

  x[i] <- y[j];